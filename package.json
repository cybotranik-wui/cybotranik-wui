{
  "name": "cybotranik-wui",
  "version": "1.0.1",
  "description": "Website html User Interface.",
  "main": "src/cybotranik-wui.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"No test specified\" && exit 0",
    "lint": "eslint src --fix",
    "build":"terser src/cybotranik-wui.js -o src/cybotranik-wui.min.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commitmsg": "commit stage",
      "pre-push": "npm run test && npm version patch"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run test",
      "npm run build",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cybotranik-wui/cybotranik-wui.git"
  },
  "keywords": [
    "cybotranik",
    "ui",
    "web",
    "user",
    "interface",
    "html5",
    "js"
  ],
  "author": "azmisahin@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cybotranik-wui/cybotranik-wui/issues"
  },
  "homepage": "https://github.com/cybotranik-wui/cybotranik-wui#readme",
  "devDependencies": {
    "eslint": "^6.2.2",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "terser": "^4.2.1"
  }
}
